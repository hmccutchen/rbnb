
<h1>Add your dog</h1>
<div class="banner banner-form" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('https://images.unsplash.com/photo-1519137847-189aca33af2b?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=21eff8fb6e8bee9d55679ddc727f9dac&auto=format&fit=crop&w=800&q=60');">

  <div>
    <%= simple_form_for(@dog) do |f| %>
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :address %>
    <%= f.input :breed %>
    <%= f.input :description %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.button :submit %>
  <%= render "reservations/go_back" %>
    <% end %>
  </div>
</div>

