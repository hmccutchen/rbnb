      <div class="container">
        <h2>Edit your Dog</h2>
      <div class="banner-form">
      <%= simple_form_for(@dog) do |f| %>
        <%= f.input :name %>
        <%= f.input :age %>
        <%= f.input :breed %>
        <%= f.input :description %>
        <%= f.button :submit %>
      <%= render "/reservations/go_back" %>
      <% end %>
      </div>
      </div>
